import "Srl/Console";
use Srl;


func swapUsingXOR(a: Int, b: Int){
    if a!= b{
     a ^= b;
     b ^= a;
     a ^= b;
    }
}


def x: Int[5];
def y: Int[10];


    Console.print("Before swapping: x = %d, y = %d\n", x, y);
    
    swapUsingXOR(&x, &y);
    
    
    Console.print("After swapping: x = %d, y = %d\n", x, y);
    
    return 0;
    
    
